package uni.fmi;

public class HW_Week2_Task1 {
    public static void main(String[] args) {
        int side_a = 2;
        int side_b = 6;

        int perimeter = 2 * side_a + 2 * side_b;
        int area = side_a * side_b;
        System.out.println(area);
        System.out.println(perimeter);
    }
}

